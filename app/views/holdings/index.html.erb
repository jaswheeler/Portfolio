<h3>Holdings</h3>

<table>
  <tr>
    <th>Symbol</th>
    <th>Price</th>
    <th>Qty</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @holdings.each do |holding| %>
  <tr>
    <td><%= holding.symbol %></td>
    <td><%= holding.price %></td>
    <td><%= holding.qty %></td>
    <td><%= link_to 'Show', holding %></td>
    <td><%= link_to 'Edit', edit_holding_path(holding) %></td>
    <td><%= link_to 'Destroy', holding, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Holding', new_holding_path %>
